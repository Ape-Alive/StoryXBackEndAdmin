    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "提供商管理"
    }
  ],
  "paths": {
    "/api/providers": {
      "get": {
        "summary": "获取提供商列表",
        "deprecated": false,
        "description": "",
        "tags": [
          "提供商管理"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "页码",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "每页数量",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 20
            }
          },
          {
            "name": "name",
            "in": "query",
            "description": "提供商标识",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "displayName",
            "in": "query",
            "description": "显示名称",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "isActive",
            "in": "query",
            "description": "是否启用",
            "required": false,
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "获取成功",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Success"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "pagination": {
                          "$ref": "#/components/schemas/Pagination"
                        }
                      }
                    }
                  ]
                }
              }
            },
            "headers": {}
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      },
      "post": {
        "summary": "创建提供商（仅超级管理员）",
        "deprecated": false,
        "description": "",
        "tags": [
          "提供商管理"
        ],
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name",
                  "displayName",
                  "baseUrl"
                ],
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "提供商标识（唯一）",
                    "example": "DeepSeek"
                  },
                  "displayName": {
                    "type": "string",
                    "description": "显示名称",
                    "example": "DeepSeek AI"
                  },
                  "baseUrl": {
                    "type": "string",
                    "format": "uri",
                    "description": "API 服务基地址",
                    "example": "https://api.deepseek.com"
                  },
                  "website": {
                    "type": "string",
                    "format": "uri",
                    "description": "官网链接",
                    "example": "https://www.deepseek.com"
                  },
                  "logoUrl": {
                    "type": "string",
                    "format": "uri",
                    "description": "Logo 链接"
                  },
                  "description": {
                    "type": "string",
                    "description": "描述信息"
                  },
                  "isActive": {
                    "type": "boolean",
                    "default": true,
                    "description": "是否启用"
                  },
                  "quota": {
                    "type": "number",
                    "format": "float",
                    "description": "额度",
                    "example": 10000
                  },
                  "quotaUnit": {
                    "type": "string",
                    "enum": [
                      "points",
                      "yuan",
                      "usd"
                    ],
                    "description": "额度单位（points=积分, yuan=元, usd=美元）",
                    "example": "points"
                  },
                  "mainAccountToken": {
                    "type": "string",
                    "description": "主账户token",
                    "example": "sk-xxxxx"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "创建成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Success"
                }
              }
            },
            "headers": {}
          },
          "400": {
            "description": "请求参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "headers": {}
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
